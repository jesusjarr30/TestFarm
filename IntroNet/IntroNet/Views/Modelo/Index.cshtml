@model List<Modelo>


    <h1>Cervezas</h1>
    <table>

    
    <thead>
        <tr>
            <th>Nombre</th>
        </tr>
        <tr>
            <th>Marca</th>
        </tr>

    </thead>
    <tbody> 
        @foreach (var mol in Model )
        {
            <tr>
                <td>
                    @Html.DisplayFor(mbox => mol.Nombre)
                </td>
                
            </tr>
            
        }

    </tbody>
</table>
